{
  "name": "arcaea-packs-inventory",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "deploy": "turbo run deploy --filter=arcaea-packs-inventory-worker",
    "dev": "turbo run dev --parallel",
    "dev:web": "turbo run dev --filter=arcaea-packs-inventory-web --filter @shared/song-data",
    "dev:worker": "turbo run dev --filter=arcaea-packs-inventory-worker --filter @shared/song-data",
    "build": "turbo run build",
    "build:web": "turbo run build --filter=arcaea-packs-inventory-web",
    "build:worker": "turbo run build --filter=arcaea-packs-inventory-worker",
    "build:shared": "turbo run build --filter=@shared/song-data",
    "start": "turbo run dev --filter=arcaea-packs-inventory-worker",
    "test": "turbo run test",
    "cf-typegen": "turbo run cf-typegen --filter=arcaea-packs-inventory-worker",
    "lint": "oxfmt --check .",
    "lint:fix": "oxfmt --write ."
  },
  "dependencies": {
    "base64-js": "^1.5.1",
    "pinia": "^3.0.4",
    "reka-ui": "^2.7.0",
    "vue": "^3.5.24",
    "vue-i18n": "^11.2.2"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.12.4",
    "@fontsource/exo": "^5.2.8",
    "@iconify-json/fluent": "^1.2.36",
    "@intlify/unplugin-vue-i18n": "^11.0.1",
    "@rollup/plugin-yaml": "^4.1.2",
    "@types/node": "^24.10.9",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "npm-run-all2": "^8.0.4",
    "oxfmt": "^0.24.0",
    "sass-embedded": "^1.97.2",
    "turbo": "^2.5.6",
    "typescript": "^5.9.3",
    "unocss": "^66.6.0",
    "vite": "8.0.0-beta.8",
    "vitest": "4.0.17",
    "vue-tsc": "^3.1.4",
    "wrangler": "^4.51.0"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.2.5"
  },
  "packageManager": "bun@1.3.6",
  "patchedDependencies": {
    "rolldown-plugin-dts@0.21.2": "patches/rolldown-plugin-dts@0.21.2.patch"
  }
}
